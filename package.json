  "scripts": {
    "mock:proc": "cross-env PRODUCTION=1  puer -p 8200 -a _mockserver.js",
    "mock:dev": "puer -p 8200  -a _mockserver.js",
    "iso:clean": "rm -rf ./client",
    "iso:build": "npm run iso:clean && cross-env BUILD_PRODUCTION=1 BUILD_ISO=1 atool-build --config webpack.iso.config.js --no-compress --output-path client --watch",
    "iso:run:app": "node ./app.js",
    "build:nc": "atool-build --no-compress --output-path client",
    "build": "cross-env BUILD_PRODUCTION=123 atool-build --output-path client",
    "build:dll": "webpack --config webpack.dll.config.js",
    "lint": "eslint --ext .js,.jsx src",
    "start": "dora  --plugins   \"webpack?disableNpmInstall,proxy?port=8002,hmr,livereload?enableJs=false&injectHost=127.0.0.1,browser-history?index=/src/entries/index.html\"",
    "server": "nodemon ./server/app.js",
    "server:unit": "node ./server/app.js",
    "ie": "dora  --plugins   \"webpack?disableNpmInstall,proxy?port=8002,livereload?enableJs=false&injectHost=127.0.0.1,browser-history?index=/src/entries/index.html\" ",
    "test:client": "cross-env __CONTEXT__=http://127.0.0.1:8001 atool-test-mocha --compilers js:babel-core/register --require ./test/setup.js --recursive  --watch --inspect",
    "test:nms": "nodemon --exec \"mocha -r ./server/test.setup.js  ./server/**/*.spec.js  --inspect  \"",
    "test:server": "cross-env UNIT_TEST_SERVER=1 NODE_ENV=development mocha -r ./server/test.setup.js ./server/**/*.spec.js --recursive  --inspect -w "
  }
